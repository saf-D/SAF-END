{
  "name": "cursor-effects",
  "type": "module",
  "description": "Old-school cursor effects for your browser built in modern JavaScript.",
  "version": "1.0.7",
  "main": "./dist/cjs.cjs",
  "module": "./dist/esm.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/cjs.cjs",
      "import": "./dist/esm.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tholman/cursor-effects.git"
  },
  "author": "tholman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tholman/cursor-effects/issues"
  },
  "homepage": "https://github.com/tholman/cursor-effects#readme",
  "devDependencies": {
    "rollup": "^2.77.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublish": "npm run build"
  }
}
